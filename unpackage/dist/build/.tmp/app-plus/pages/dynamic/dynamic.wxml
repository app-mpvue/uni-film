<view class="content"><swiper class="carousel" indicator-dots="{{true}}" autoplay="{{true}}"><swiper-item><image class="carousel" src="../../static/dongtai/1.jpg"></image></swiper-item><swiper-item><image class="carousel" src="../../static/dongtai/2.jpg"></image></swiper-item><swiper-item><image class="carousel" src="../../static/dongtai/3.jpg"></image></swiper-item><swiper-item><image class="carousel" src="../../static/dongtai/4.jpg"></image></swiper-item><swiper-item><image class="carousel" src="../../static/dongtai/5.jpg"></image></swiper-item></swiper><block wx:for="{{userList}}" wx:for-item="userObj" wx:for-index="userIndex" wx:key="userIndex"><view class="commit-items"><view class="commit-user"><image class="commit-user-headphoto" src="{{userObj.imgs[userIndex]}}"></image><view class="commit-user-name">{{userObj.username}}</view><button class="{{[userObj.userId>-1?'user-unattention':'user-isattention']}}" plain="true" size="default" hover-class="none" data-event-opts="{{[['tap',[['clickattention',['$0'],[[['userList','',userIndex,'userId']]]]]]]}}" bindtap="__e">+关注</button></view><view class="commit-detail"><navigator url="{{'comment/comment?userId='+userObj.userId}}"><view class="commit-detail-text">{{userObj.text}}</view></navigator><view class="commit-detail-img"><block wx:for="{{userObj.imgs}}" wx:for-item="deTail" wx:for-index="imgIndex" wx:key="imgIndex"><view><image class="commit-detail-imgs" src="{{deTail}}" mode="aspectFill" data-imgIndex="{{imgIndex}}" data-userIndex="{{userIndex}}" data-event-opts="{{[['tap',[['lookMe',['$event']]]]]}}" bindtap="__e"></image></view></block></view></view><view class="commit-bottom"><view class="commit-bottom-time">x小时前</view><view class="like-commit"><view data-event-opts="{{[['tap',[['selectLike',['$0'],[[['userList','',userIndex,'userId']]]]]]]}}" class="{{[userObj.like?'commit-bottom-like':'commit-bottom-unlike']}}" bindtap="__e"></view><view class="commit-bottom-like-num">{{userObj.likeNum}}</view><navigator url="{{'comment/comment?userId='+userObj.userId}}"><view class="comment"><image class="commit-bottom-commit" src="../../static/dongtai/commit.png"></image><view class="commit-bottom-commit-num">{{userObj.commentNum}}</view></view></navigator></view></view></view></block></view>