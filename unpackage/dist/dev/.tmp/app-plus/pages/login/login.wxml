<view class="content"><view class="titles"><image class="logo" src="../../static/alpra.png"></image><view class="text-area"><text class="title">{{title}}</text></view></view><view class="inputText"><input class="account" type="text" placeholder="输入账号" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]],['onInput',['$event']]]]]}}" value="{{account}}" bindinput="__e"/><image class="line" src="../../static/Unknown.jpeg"></image><view class="pwdInput"><input class="pwd" type="password" placeholder="输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]],['onInput',['$event']]]]]}}" value="{{password}}" bindinput="__e"/><image class="verticalLine" src="../../static/Unknown.jpeg"></image><view class="forget"><text class="forgetIt">forget password?</text></view></view><image class="line" src="../../static/Unknown.jpeg"></image></view><view class="btnArea"><button data-event-opts="{{[['tap',[['onClickListener',['$event']]]]]}}" class="signInBtn" bindtap="__e"><text class="signinTxt">LOGIN</text></button><text data-event-opts="{{[['tap',[['onRegisterListener',['$event']]]]]}}" class="signUpTxt" bindtap="__e">SIGN UP</text></view></view>